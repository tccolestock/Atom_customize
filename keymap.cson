  'ctrl-k k': 'pane:split-down-and-copy-active-item'
  'ctrl-k i': 'pane:split-up-and-copy-active-item'
  'ctrl-k l': 'pane:split-right-and-copy-active-item'
  'ctrl-k j': 'pane:split-left-and-copy-active-item'
  'ctrl-alt-j': 'window:focus-pane-on-left'
  'ctrl-alt-l': 'window:focus-pane-on-right'
  'ctrl-alt-i': 'window:focus-pane-above'
  'ctrl-alt-k': 'window:focus-pane-below'

  'f4': 'application:new-file'
  'ctrl-shift-f4': 'application:add-project-folder'
  'f6': 'core:close'
  'shift-f6': 'pane:close'
  'ctrl-shift-f6': 'window:close'
  # 'f2': 'core:undo'
  'shift-delete': 'editor:delete-line'



'atom-text-editor':
  'ctrl-shift-A': 'git-plus:add'
  'ctrl-shift-X': 'git-plus:commit'
  'ctrl-shift->': 'git-plus:push'
  'ctrl-shift-<': 'git-plus:pull'
  'shift-delete': 'editor:delete-line'



'.platform-linux, .platform-linux .command-palette atom-text-editor':
  'f1': 'command-palette:toggle'


'.platform-linux atom-text-editor':
  'ctrl-shift-f10': 'bookmarks:toggle-bookmark'
  'alt-shift-f10': 'bookmarks:clear-bookmarks'


'atom-text-editor':
  'f10': 'bookmarks:jump-to-next-bookmark'
  'ctrl-f10': 'bookmarks:view-all'
  'shift-f10': 'bookmarks:jump-to-previous-bookmark'
